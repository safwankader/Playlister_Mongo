et.id.indexOf(\"-\") + 1);\r\n        let sourceId = event.dataTransfer.getData(\"song\");\r\n        sourceId = sourceId.substring(sourceId.indexOf(\"-\") + 1);\r\n        store.addMoveSongTransaction(parseInt(sourceId.substring(0,1)),parseInt(targetId.substring(0,1)));\r\n        \r\n\r\n        // // ASK THE MODEL TO MOVE THE DATA\r\n        // this.props.moveCallback(sourceId, targetId);\r\n    }\r\n\r\n\r\n\r\n\r\n    let cardClass = \"list-card unselected-list-card\";\r\n    return (\r\n        <div\r\n            key={index}\r\n            id={'song-' + index + '-card'}\r\n            className={cardClass}\r\n            onDragStart={handleDragStart}\r\n            onDragOver={handleDragOver}\r\n            onDragEnter={handleDragEnter}\r\n            onDragLeave={handleDragLeave}\r\n            onDrop={handleDrop}\r\n            onDoubleClick={handleEditSong}\r\n            draggable=\"true\"\r\n        >\r\n            {index + 1}.\r\n            <a\r\n                id={'song-' + index + '-link'}\r\n                className=\"song-link\"\r\n                href={\"https://www.youtube.com/watch?v=\" + song.youTubeId}>\r\n                {song.title} by {song.artist}\r\n            </a>\r\n            <input\r\n                type=\"button\"\r\n                id={\"remove-song-\" + index}\r\n                className=\"list-card-button\"\r\n                onClick={handleDeleteSong}\r\n                value={\"\\u2715\"}\r\n            />\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default SongCard;","C:\\Users\\skade\\Playlister_HW3\\client\\src\\components\\DeleteModal.js",[],"C:\\Users\\skade\\Playlister_HW3\\client\\src\\components\\DeleteSongModal.js",[],"C:\\Users\\skade\\Playlister_HW3\\client\\src\\components\\EditSongModal.js",[],"C:\\Users\\skade\\Playlister_HW3\\client\\src\\transactions\\AddSong_Transaction.js",[],"C:\\Users\\skade\\Playlister_HW3\\client\\src\\transactions\\MoveSong_Transaction.js",[],"C:\\Users\\skade\\Playlister_HW3\\cl